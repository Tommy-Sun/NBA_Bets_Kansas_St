{
    "version": 2,
    "builds": [
        {
            "src": "vercel_build_staticfiles.sh",
            "use": "@vercel/static-build",
            "config": {
              "distDir": "staticfiles_build"
            }
          },
        {
        "src": "NBA_Bet_Kansas_St/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "35mb" }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
          },
        {
            "src": "/(.*)",
            "dest": "NBA_Bet_Kansas_St/wsgi.py"
        }
    ]
}